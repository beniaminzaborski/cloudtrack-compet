<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Version>$(CompetitionVersion)</Version>
    <PackageId>CloudTrack.Competitions.Messaging</PackageId>
    <Authors>Beniamin Zaborski</Authors>
    <Company>Beniamin Zaborski</Company>
    <Description>This package contains CloudTrack Competitions integration events</Description>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    <LangVersion>latest</LangVersion>
  </PropertyGroup>
</Project>